    function appendOption(input_name, name, offset){
        var sel=document.getElementById(input_name);
        if (sel) {
            if ((sel.size - 1) >= offset) {
                sel.options[offset].text=name;
                sel.options[offset].value=name;
                sel.options[offset].selected=false;
            } else {
                var option=document.createElement("option");
                option.text=name;
                option.value=name;
                try{
                  // for IE earlier than version 8
                  sel.add(option,sel.options[null]);
                } catch (e) {
                  sel.add(option,null);
                }
                sel.size = sel.length;
                sel.multiple = "multiple";
            }
        } else {
           //alert("No select input for " + input_name);
        }
    }


   function calculateTotal(){
       var amount = document.getElementById("input_197").value;
       var totalTextBox = document.getElementById("input_202");
       if (amount) {
           var multiplier = 52;
           var frequencySel = document.getElementById("input_198").selectedIndex;
           if (frequencySel == 1) {
               multiplier = 12;
           } else if (frequencySel == 2) {
               multiplier = 1;
           } else {
               multiplier = 52;
           }
           
           var totalValue = Math.round((amount*multiplier)*100)/100;

           totalTextBox.value = "$ " + totalValue.toFixed(2).replace(/(\d)(?=(\d{3})+\.)/g, "$1,");
       } else {
         totalTextBox.value = "";
       }
   
   }

   function formatBankTotal(){
      var bankTextBox = document.getElementById("input_206");
            var amount = bankTextBox.value;
            if (amount) {
                //  it does not contain dollar sign, then format to money value
                if (amount.indexOf("$") == -1) {
                   var totNumber = Math.round((amount)*100)/100;
                   if (Number(amount)) {
                     bankTextBox.value = "$ " + totNumber.toFixed(2).replace(/(\d)(?=(\d{3})+\.)/g, "$1,");
                   } else {
                     bankTextBox.value = "";
                   }
                }
            }
   }
   
   function formatCreditCardTotal(){
      var bankTextBox = document.getElementById("input_338");
            var amount = bankTextBox.value;
            if (amount) {
                //  it does not contain dollar sign, then format to money value
                if (amount.indexOf("$") == -1) {
                   var totNumber = Math.round((amount)*100)/100;
                   if (Number(amount)) {
                     bankTextBox.value = totNumber.toFixed(2).replace(/(\d)(?=(\d{3})+\.)/g, "$1,");
                   } else {
                     bankTextBox.value = "";
                   }
                }
            }
   }
   
   // example call onchange="setPhoneFormat(this);"
   function setPhoneFormat(phoneField) {
        var phone = phoneField.value;
        var raw_number = phone.replace(/[^0-9]/g,'');
         var formatted_number = '';
        if (/^[0-9]{10}$/.test(raw_number)) {
            formatted_number = phone.replace(/(\d{3})(\d{3})(\d{4})/,'$1-$2-$3')
        } else if (/^[0-9]{7}$/.test(raw_number)) {
            formatted_number = phone.replace(/(\d{3})(\d{4})/,'502-$1-$2')
        }
        phoneField.value = formatted_number;
   }
   

