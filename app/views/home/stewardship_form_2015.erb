                   } else {
                     bankTextBox.value = "";
                   }
                }
            }
   }

   // example call onchange="setPhoneFormat(this);"
   function setPhoneFormat(phoneField) {
        var phone = phoneField.value;
        var raw_number = phone.replace(/[^0-9]/g,'');
         var formatted_number = '';
        if (/^[0-9]{10}$/.test(raw_number)) {
            formatted_number = phone.replace(/(\d{3})(\d{3})(\d{4})/,'$1-$2-$3')
        } else if (/^[0-9]{7}$/.test(raw_number)) {
            formatted_number = phone.replace(/(\d{3})(\d{4})/,'502-$1-$2')
        }
        phoneField.value = formatted_number;
   }


